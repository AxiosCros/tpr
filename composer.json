{
  "name": "axios/tpr",
  "description": "The framework for quickly developing cgi&cli applications",
  "keywords": [
    "framework",
    "Api",
    "cgi",
    "cli"
  ],
  "homepage": "https://github.com/AxiosCros/tpr",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "axios",
      "email": "axioscros@aliyun.com"
    }
  ],
  "support": {
    "issues": "https://github.com/AxiosCros/tpr/issues"
  },
  "require": {
    "php": ">=7.4",
    "filp/whoops": "^2.3",
    "hassankhan/config": "^2.0",
    "nette/php-generator": "^3.3",
    "nikic/fast-route": "^1.3",
    "rakit/validation": "^1.3",
    "ralouphie/mimey": "^2.1",
    "symfony/console": "^4.3",
    "symfony/yaml": "^4.2",
    "twig/twig": "^2.11",
    "webmozart/path-util": "^2.3"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.16",
    "phpunit/phpunit": "^7",
    "swoft/swoole-ide-helper": "^4.4",
    "symfony/var-dumper": "*"
  },
  "autoload": {
    "psr-4": {
      "tpr\\": "src/",
      "tpr\\tests\\": "tests/"
    },
    "files": [
      "global.php"
    ]
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": [
      "phpunit --colors=always"
    ],
    "fix": [
      "./vendor/bin/php-cs-fixer fix ./"
    ]
  }
}
